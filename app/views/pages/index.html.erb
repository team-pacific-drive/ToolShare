<% if user_signed_in? %>
  <%= react_component "AuthenticatedApp", current_user: {
    id: current_user.id,
    email: current_user.email,
    firstname: current_user.firstname,
    lastname: current_user.lastname,
    tool_ids: current_user.tool_ids,
  } %>
<% else %>
  <%= react_component 'UnauthenticatedApp', props: {'logged_in': false}%>
<% end %>
